{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "indent": [
      "off",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-console": [
      "off"
    ],
    "no-unused-vars": ["error", {
      "vars": "all",
      "args": "none"
    }]
  },
  "ecmaFeatures": {
    "experimenta  lObjectRestSpread": true
  },
  "globals": {
    "angular": false
  }
}
